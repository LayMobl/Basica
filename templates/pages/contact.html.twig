{#
  ./templates/pages/contact.html.twig
  Variables disponibles:
    - Page(id, titrefr, titreen, slugfr, slugen)
#}

<div class="section section-breadcrumbs">
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <h1>{{ attribute(page, 'titre' ~ app.request.locale) }}</h1>
      </div>
    </div>
  </div>
</div>

<div class="section section-map">

  <div class="col-sm-12" style="padding:0;">
    <!-- Map -->
    <div id="contact-us-map">
      <iframe
        width="100%"
        height="100%"
        frameborder="0"
        scrolling="no"
        marginheight="0"
        marginwidth="0"
        src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d10127.427348940084!2d5.6826843!3d50.6111977!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x2d5b3cb7b45da7ab!2sIeps+Fl%C3%A9ron!5e0!3m2!1sen!2sbe!4v1556364081325!5m2!1sen!2sbe"></iframe>"></iframe>
      <br/>
      <small>
        <a
          href="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d10127.427348940084!2d5.6826843!3d50.6111977!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x2d5b3cb7b45da7ab!2sIeps+Fl%C3%A9ron!5e0!3m2!1sen!2sbe!4v1556364081325!5m2!1sen!2sbe"></a>
      </small>
    </iframe>
  </div>
  <!-- End Map -->
</div>

</div>

<div class="section">
<div class="container">
  <div class="row">
    <h3>{{ 'contact.titre' | trans }}</h3>
  </hr>
  <div class="col-sm-6">
    <!-- Contact Info -->
    <p class="contact-us-details">
      <b>{{ 'footer.adresse' | trans }}:</b> Rue Charles Deliège 9, 4623 Fléron<br/>
      <b>Tel:</b> +123456789<br/>
      <b>Fax:</b> +123456789<br/>
      <b>Email:</b> <a href="mailto:getintouch@yourcompanydomain.com">getintouch@ieps.com</a>
    </p>
    <!-- End Contact Info -->
  </div>
  <div class="col-sm-6"></div>
</div>
</div>
</div>
