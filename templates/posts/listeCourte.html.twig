{#
  ./templates/posts/show.html.twig
  Variables disponibles:
    - posts Array (id, nomfr, nomen, titreonefr, titreoneen, titretwofr, titretwoen, texteonefr, texteoneen, textetwofr, textetwoen, date, imageName, Array tags)
#}



  <ul class="recent-posts">
  {% for post in posts %}
    <li><a href="{{path('app_posts_show', {id: post.id , slug: attribute(post, 'nom' ~ app.request.locale) | slugify})}}">{{ attribute(post, 'nom' ~ app.request.locale) }}</a></li>
  {% endfor %}
  </ul>
